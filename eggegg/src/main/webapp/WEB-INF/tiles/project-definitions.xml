<?xml version="1.0" encoding="ISO-8859-1" ?>
<!DOCTYPE tiles-definitions PUBLIC
       "-//Apache Software Foundation//DTD Tiles Configuration 3.0//EN"
       "http://tiles.apache.org/dtds/tiles-config_3_0.dtd">
<tiles-definitions>
  <definition name="project/*" extends="abstractParent">
    <put-attribute name="content" value="/WEB-INF/views/project/{1}.jsp" />
    <put-attribute name="leftMenu" value="/includee/emplMenu.jsp" />
  </definition>
  <definition name="dept/*" extends="abstractParent">
    <put-attribute name="content" value="/WEB-INF/views/dept/{1}.jsp" />
    <put-attribute name="leftMenu" value="/includee/emplMenu.jsp" />
  </definition>
</tiles-definitions>


